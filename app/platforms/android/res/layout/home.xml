<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/beige_background" >

    <TableLayout
        android:id="@+id/homeTable"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:shrinkColumns="0,1,2"
        android:stretchColumns="0,1,2" >

        <TableRow>

            <FrameLayout
                android:id="@+id/Frame7"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView7"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView7"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText7"
                    style="@style/plusText" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/Frame6"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView6"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView6"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText6"
                    style="@style/plusText" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/Frame3"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView3"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView3"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText3"
                    style="@style/plusText" />
            </FrameLayout>
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/nameText7"
                style="@style/nameText" />

            <TextView
                android:id="@+id/nameText6"
                style="@style/nameText" />

            <TextView
                android:id="@+id/nameText3"
                style="@style/nameText" />
        </TableRow>

        <TableRow>

            <FrameLayout
                android:id="@+id/Frame5"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView5"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView5"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText5"
                    style="@style/plusText" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/camera_preview_frame"
                style="@style/vFrame" >

                <com.noplanbees.tbm.CameraOverlay
                    android:id="@+id/camera_overlay_surface"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" />

                <com.noplanbees.tbm.CameraPreview
                    android:id="@+id/camera_preview_surface"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/Frame0"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView0"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView0"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText0"
                    style="@style/plusText" />
            </FrameLayout>
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/nameText5"
                style="@style/nameText" />

            <TextView
                android:id="@+id/previewText"
                style="@style/nameText" />

            <TextView
                android:id="@+id/nameText0"
                style="@style/nameText" />
        </TableRow>

        <TableRow>

            <FrameLayout
                android:id="@+id/Frame4"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView4"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView4"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText4"
                    style="@style/plusText" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/Frame2"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView2"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView2"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText2"
                    style="@style/plusText" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/Frame1"
                style="@style/vFrame" >

                <ImageView
                    android:id="@+id/ThumbView1"
                    style="@style/videoView" />

                <VideoView
                    android:id="@+id/VideoView1"
                    style="@style/videoView" />

                <TextView
                    android:id="@+id/PlusText1"
                    style="@style/plusText" />
            </FrameLayout>
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/nameText4"
                style="@style/nameText" />

            <TextView
                android:id="@+id/nameText2"
                style="@style/nameText" />

            <TextView
                android:id="@+id/nameText1"
                style="@style/nameText" />
        </TableRow>
    </TableLayout>

    <RelativeLayout
        android:id="@+id/bench_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/charcoal" >

        <com.noplanbees.tbm.ContactsAutoCompleteTextView
            android:id="@+id/contacts_auto_complete_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:completionThreshold="1"
            android:ems="10"
            android:hint="Search"
            android:textColor="@color/white" />

        <ListView
            android:id="@+id/bench_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_below="@id/contacts_auto_complete_text_view" >

            <requestFocus />
        </ListView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/select_phone_number_frame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible" 
        android:background="@color/charcoal"
        android:textColor="@color/white">

        <TextView
            android:id="@+id/select_phone_number_title"
            style="@style/dialog_title"
            android:layout_alignParentTop="true"/>
        
        <ListView
            android:id="@+id/select_phone_number_list_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/select_phone_number_title"
            android:layout_alignParentBottom="true"
            />
    </RelativeLayout>

</RelativeLayout>